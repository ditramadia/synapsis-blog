{
  "extends": ["next/core-web-vitals", "eslint:recommended"],
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "rules": {
    "semi": ["error", "never"],
    "no-unused-vars": "warn",
    "no-console": "warn",
    "exhaustive-deps": "never",
    "object-curly-spacing": ["error", "always"]
  }
}
